<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Sonata\UserBundle\Entity\DateTimeClassroom" table="date_time_classroom">
    <unique-constraints>
      <unique-constraint name="date_time_classroom_classroom_UNIQUE" columns="date,time_id,classroom_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="date" column="date" nullable="true"/>
    <many-to-one field="time_id" target-entity="Timeforclassroom" fetch="LAZY">
      <join-columns>
        <join-column name="time_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="classroom_id" target-entity="Classroom" fetch="LAZY">
      <join-columns>
        <join-column name="classroom_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

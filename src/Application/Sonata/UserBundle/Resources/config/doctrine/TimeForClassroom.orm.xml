<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Application\Sonata\UserBundle\Repository\TimeForClassroomRepository" name="Application\Sonata\UserBundle\Entity\TimeForClassroom">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="title" type="string" column="title" length="255"/>
    <field name="startTime" type="time" column="start_time"/>
    <field name="endTime" type="time" column="end_time"/>
  </entity>
</doctrine-mapping>

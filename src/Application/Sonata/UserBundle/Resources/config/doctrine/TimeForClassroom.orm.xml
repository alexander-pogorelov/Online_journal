<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Sonata\UserBundle\Entity\Timeforclassroom" table="TimeForClassroom">
    <unique-constraints>
      <unique-constraint name="title_UNIQUE" columns="title"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="startTime" type="time" column="start_time" nullable="false"/>
    <field name="endTime" type="time" column="end_time" nullable="false"/>
  </entity>
</doctrine-mapping>

namespace Application\Sonata\UserBundle\Entity;


class TeacherSubject
{
    private $id;

    private $teacher;

    private $subject;

    public function __construct(UserTeacher $teacher, Subject $subject)
    {
        $this->subject = $subject;
        $this->teacher = $teacher;
    }
}